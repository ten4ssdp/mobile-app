{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint './**/*.{js,jsx}'",
    "lint:fix": "eslint './**/*.{js,jsx}' --fix",
    "test": "jest --coverage"
  },
  "preset": "react-native",
  "setupFilesAfterEnv": [
    "@testing-library/jest-native/extend-expect"
  ],
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-cookies)/)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.6",
    "@react-navigation/native": "^5.3.0",
    "@react-navigation/stack": "^5.3.3",
    "expo": "^37.0.0",
    "expo-font": "~8.1.0",
    "jwt-decode": "^2.2.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-calendars": "^1.265.0",
    "react-native-error-boundary": "^1.1.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-reanimated": "~1.7.0",
    "react-native-redash": "^14.1.1",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "~2.2.0",
    "react-native-web": "^0.11.7",
    "react-navigation-header-buttons": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@testing-library/jest-native": "^3.1.0",
    "babel-preset-expo": "^8.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-universe": "^3.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-expo": "^37.0.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.0.5",
    "react-native-testing-library": "^2.0.1",
    "react-test-renderer": "^16.13.1"
  },
  "private": true
}
